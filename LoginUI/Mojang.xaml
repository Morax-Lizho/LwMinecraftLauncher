<Page x:Class="LwMinecraftLauncher.LoginUI.Mojang"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:LwMinecraftLauncher.LoginUI"
      mc:Ignorable="d" 
      d:DesignHeight="200" d:DesignWidth="200"
      Title="Mojang">

    <Grid>
        <PasswordBox x:Name="Password" Height="23" Margin="64,0,0,0" VerticalAlignment="Bottom"/>
        <TextBox x:Name="Email" Height="23" Margin="64,0,0,28" TextWrapping="Wrap" Text="" VerticalAlignment="Bottom"/>
        <Label Content="密码:" HorizontalAlignment="Left" VerticalAlignment="Bottom" Height="30" Width="59" FontSize="16" Margin="5,0,0,0"/>
        <Label Content="账号:" HorizontalAlignment="Left" Margin="5,0,0,23" VerticalAlignment="Bottom" Height="28" Width="59" FontSize="16"/>

    </Grid>
</Page>
